{
  "title": "Open Alacritty Terminal window with Ctrl-Cmd-T",
  "rules": [
    {
        "description": "Open Alacritty Terminal window with Ctrl-Cmd-T",
        "manipulators": [
            {
                "from": {
                    "key_code": "t",
                    "modifiers": {
                        "mandatory": [
                            "left_control",
                            "left_command"
                        ],
                        "optional": [
                            "caps_lock"
                        ]
                    }
                },
                "to": [
                    {
                        "shell_command": "open -a Alacritty -n"
                    }
                ],
                "type": "basic"
            }
        ]
    }
  ]
}

